<%= form_for [:admin, @location], :method => :put do |f| %>
  <table class="basic-table">
    <%= render :partial => "form", :locals => { :f => f } %>
  </table>
  <p class="form-buttons">
    <%= button t("update") %>
    <%= t('or') %> <%= link_to t("cancel"), admin_locations_url, :id => "cancel_link" %>
  </p>
<% end %>
